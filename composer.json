{
    "name"              : "jbzoo/profiler",
    "type"              : "library",
    "description"       : "Simple Profiler for PHP code and unit tests",
    "license"           : "MIT",
    "keywords"          : [
        "profiler", "performance", "phpunit", "tests", "benchmark", "timer", "microtime",
        "memory", "leak", "memoryleak"
    ],
    "minimum-stability" : "stable",
    "authors"           : [
        {
            "name"  : "SmetDenis",
            "email" : "denis@jbzoo.com",
            "role"  : "lead"
        }
    ],
    "require"           : {
        "php"         : ">=5.3",
        "jbzoo/utils" : "^1.6"
    },
    "require-dev"       : {
        "jbzoo/phpunit" : "^1.7"
    },
    "autoload"          : {
        "psr-4" : {
            "JBZoo\\Profiler\\" : "src"
        }
    },
    "extra"             : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    },
    "config"            : {
        "optimize-autoloader" : true
    },
    "support"           : {
        "issues" : "https://github.com/JBZoo/Profiler/issues"
    }
}
